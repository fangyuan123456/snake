  "homepage": "https://github.com/brandonxiang/pb-to-typescript#readme",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./src/index.js"
    },
    "./core": {
      "types": "./types/core.d.ts",
      "import": "./src/core.js"
    }
  },
  "types": "./types/index.d.ts",
  "type": "module",
  "directories": {
    "src": "src",
    "test": "tests",
    "types": "types"
  },
  "files": [
    "src",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonxiang/pb-to-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/brandonxiang/pb-to-typescript/issues"
  },
  "dependencies": {
    "protobufjs": "^7.2.3",
    "magic-string": "^0.30.0",
    "fs-extra": "11.1.1",
    "sade": "^1.7.4",
    "pkg-updater": "^1.0.4"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.1",
    "@types/node": "^17.0.36",
    "@types/sade": "^1.7.4",
    "typescript": "^5.0.4",
    "uvu": "^0.5.6",
    "ts-unused-exports": "^9.0.4"
  },
  "bin": {
    "pbts": "bin.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "check": "tsc",
    "tsd": "cp src/type.d.ts types/type.d.ts && tsc ----emitDeclarationOnly",
    "test": "uvu tests"
  }
}